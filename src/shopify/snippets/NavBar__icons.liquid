{%- comment -%}
This is just a recommended starting point...customize away!
Note: this is only used when the pre-header is disabled. If you have a pre-header, you'll want to modify snippets/PreHeader__icons.liquid instead.
{%- endcomment -%}
<div class="NavBar__icons">
  {% comment %} <a class="Button Button--primary" onclick="fbq('track', 'Lead');" href="/pages/quiz">Take our survey</a> {% endcomment %}
  <span class="NavBar__text">Feeling Sick?</span>
  <a class="Button Button--primary" href="/pages/quiz">Get Relief</a>
  {% comment %} <a class="Button Button--primary" href="/pages/quiz">Take the quiz</a> {% endcomment %}
  {%- if shop.customer_accounts_enabled -%}
    <a class="NavBar__icon" href="/account"><i aria-label="{{ 'customer.account.title' | t }}" class="{{ settings.fa }} fa-user"></i></a>
  {%- endif -%}
  {%- capture search_label -%}
    {% include 'INF-translation', translate: 'general.search.title', default: 'Search our store' %}
  {%- endcapture -%}
  {%- capture cart_label -%}
    {% include 'INF-translation', translate: 'cart.general.title', default: 'Shopping cart' %}
  {%- endcapture -%}
  {% comment %} <a class="NavBar__icon NavBar__icon--search">{%- include 'INF-IconToggle', icon: 'fa-search', direction: 'down', data: 'data-search-toggle', aria_label: search_label -%}</a> {% endcomment %}
  <a class="NavBar__icon">{%- include 'INF-IconToggle', icon: settings.cart_icon, direction: 'right', data: 'data-cart-toggle', aria_label: cart_label -%}</a>
</div>