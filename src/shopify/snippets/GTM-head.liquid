{%- if settings.gtm -%}
<!-- Google Tag Manager -->
<script async src="https://www.googletagmanager.com/gtag/js?id={{ settings.gtm }}"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', '{{ settings.gtm }}');
</script>
<!-- End Google Tag Manager -->
<!-- Conversion Event: Lead Submission -->
<script>
function gtag_report_conversion(url) {
  var callback = function () {
    if (typeof(url) != 'undefined') {
      window.location = url;
    }
  };
  gtag('event', 'conversion', {
      'send_to': '{{ settings.gtm }}/xjXXCIruudoBEIqojaEC',
      'event_callback': callback
  });
  return false;}
</script>
<!-- End Conversion Event: Lead Submission -->
{%- endif -%}